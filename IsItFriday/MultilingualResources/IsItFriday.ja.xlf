    <body>
      <group id="ISITFRIDAY/RESOURCES/VALUES/STRINGS.XML" datatype="resx">
        <trans-unit id="app_name" translate="yes" xml:space="preserve">
          <source>Is It Friday</source>
          <target state="translated">金曜日ですか</target>
        </trans-unit>
        <trans-unit id="yes" translate="yes" xml:space="preserve">
          <source>Yes</source>
          <target state="translated" state-qualifier="tm-suggestion">はい</target>
        </trans-unit>
        <trans-unit id="no" translate="yes" xml:space="preserve">
          <source>No</source>
          <target state="translated" state-qualifier="tm-suggestion">いいえ</target>
        </trans-unit>
        <trans-unit id="itsFridayToast" translate="yes" xml:space="preserve">
          <source>Woohoo! It\'s Friday!</source>
          <target state="translated">ウーホー！ それは金曜日だ！</target>
        </trans-unit>
        <trans-unit id="itsNotFridayToast" translate="yes" xml:space="preserve">
          <source>Still not Friday...</source>
          <target state="translated">まだ金曜日ではありません...</target>
        </trans-unit>
      </group>
